/* components/w-image/w-image.wxss */
.container{
  display: flex;
  flex-direction: column;
}

.titleImg{
  height: 50vh;
  position: relative;
}

.add{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  height: 100%;
  background:#A2A2A2;
}

image{
  width: 100%;
  height: 50vh;
}

.del_img{
  position: absolute;
  width:60rpx;
  height:60rpx;
  right: 30rpx;
  top:40rpx;
}

.textTitle{
  width:80%;
  text-align: center;
  border-block-end: 1rpx solid #dddddd;
  font-size:50rpx;
  padding:20rpx;
  margin:20rpx;
}

.title{
  display: flex;
  justify-content: center;
}
